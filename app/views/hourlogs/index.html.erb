<% provide(:title, 'All logs') %>

<h1>All logs</h1>

<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover dataTable" id="user_table">
<thead>
<tr role="row">
<th>User ID</th>
<th>Date</th>
<th>Study</th>
<th>Sleep</th>
<th>Lecture</th>
<th>Mandatory</th>
<th>Extracurriculars</th>
<th>Meals</th>
<th>Anxious</th>
<th>Sad</th>
<th>Depressed</th>
<th>Lack of Energy</th>
<th>Hopelessness</th>
<th>Trouble Sleeping</th>
<th>Trouble Staying Awake</th>
<th>Trouble Focusing</th>
<th>Fun</th>
<th>Exercise</th>
</tr>
</thead>
<tbody>
<% for hourlog in @hourlogs %>
<tr>
<td><%= hourlog.user_id %></td>
<td><%= hourlog.logdate %></td>
<td><%= hourlog.h_study %> hours</td>
<td><%= hourlog.h_sleep %> hours</td>
<td><%= hourlog.h_lecture %> hours</td>
<td><%= hourlog.h_mandatory %> hours</td>
<td><%= hourlog.h_extracurriculars %> hours</td>
<td><%= hourlog.meal %></td>
<td><%= hourlog.anxious == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.sad == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.depressed == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.energy == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.hopeless == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.sleep_trouble == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.awake_trouble == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.energy == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.no_school == 1 ? "Yes" : "No" %></td>
<td><%= hourlog.exercise == 1 ? "Yes" : "No" %></td>
</tr>
<% end %>
</tbody>
</table>
